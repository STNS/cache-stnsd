[Unit]
Description=stns cache server
After=syslog.target network.target

[Service]
Type=simple
PIDFile=/var/run/stnsd.pid
ExecStart= /bin/sh -c '/usr/sbin/stnsd server >> /var/log/stnsd.log 2>&1'

KillSignal=SIGINT
KillMode=control-group
Restart=always
User=root
Group=root

[Install]
WantedBy=multi-user.target
